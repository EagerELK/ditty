.row
  .col-md-2
  .col-md-8
    .panel.panel-default
      .panel-body
        = edit_form_tag "#{base_path}/#{entity.id}" do
          = haml :'<%= model_name.pluralize.underscore %>/form', locals: { entity: entity }
          %button.btn.btn-primary{ type: 'submit' }
            Update <%= model_name %>
  .col-md-2
